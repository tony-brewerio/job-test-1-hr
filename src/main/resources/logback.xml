<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d] [%thread] [%5level] [%logger{36}] %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.github.javafaker" level="WARN"/>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
